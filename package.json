{
  "name": "watashime",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "start": "bun ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && prettier . --write",
    "scrape:slugs": "bun ./scripts/scrape-slug.ts",
    "scan": "bun ./scripts/scan-dom.ts"
  },
  "dependencies": {
    "@upstash/redis": "^1.35.4",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^17.2.2",
    "hono": "^4.9.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bun": "latest",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.44.0"
  }
}
